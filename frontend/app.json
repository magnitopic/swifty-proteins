{
	"expo": {
		"name": "swifty-proteins",
		"slug": "swifty-proteins",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"plugins": [
			[
				"expo-local-authentication",
				{
				"faceIDPermission": "Let us use FaceID for a faster login."
				}
			]
		],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.42.swiftyproteins"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.42.swiftyproteins"
		},
		"web": {
			"favicon": "./assets/favicon.png",
			"bundler": "metro"
		}
	}
}
